{
  "properties": [
    {
      "name": "mcn.bootstrap.eagerLoad.enable",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "是否在引导上下文中加载.",
      "defaultValue": false
    },
    {
      "name": "mcn.log.file.enable",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "是否开启日志文件配置.",
      "defaultValue": true
    },
    {
      "name": "mcn.log.file.filter-level",
      "type": "java.lang.String",
      "description": "日志文件输出过滤器级别.",
      "defaultValue": "info"
    },
    {
      "name": "mcn.log.file.name",
      "type": "java.lang.String",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "日志文件名称(仅仅文件名,不含路径).",
      "defaultValue": "error"
    },
    {
      "name": "filter.cross",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.web.filter.FilterAutoConfiguration",
      "description": "是否开启跨域支持.",
      "defaultValue": false
    },
    {
      "name": "swagger.path.ignore",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.web.mvc.SpringMvcAutoConfiguration$Swagger$IgnoreSwaggerPath",
      "description": "是否开启忽略swagger路径.",
      "defaultValue": true
    }
  ]
}