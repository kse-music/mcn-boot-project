{
  "properties": [
    {
      "name": "mcn.bootstrap.eagerLoad.enable",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "是否在引导上下文中加载.",
      "defaultValue": false
    },
    {
      "name": "mcn.log.file.eagerLoad",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "是否早期就加载默认日志文件配置.",
      "defaultValue": false
    },
    {
      "name": "mcn.log.file.filter-level",
      "type": "java.lang.String",
      "description": "日志文件输出过滤器级别.",
      "defaultValue": "info"
    },
    {
      "name": "mcn.log.file.name",
      "type": "java.lang.String",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnPropertiesPostProcessor",
      "description": "日志文件名称(仅仅文件名,不含路径).",
      "defaultValue": "error"
    },
    {
      "name": "filter.cross",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.web.filter.FilterAutoConfiguration",
      "description": "是否开启跨域支持.",
      "defaultValue": false
    },
    {
      "name": "mcn.xss.enable",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.web.filter.FilterAutoConfiguration",
      "description": "是否开启XSS过滤.",
      "defaultValue": false
    },
    {
      "name": "mcn.print-env.enable",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.context.McnApplicationListener",
      "description": "是否打印环境配置.",
      "defaultValue": false
    },
    {
      "name": "http.code.override",
      "type": "java.lang.Boolean",
      "sourceType": "cn.hiboot.mcn.autoconfigure.web.mvc.ErrorPageController",
      "description": "是否重写http状态码.",
      "defaultValue": true
    }
  ]
}